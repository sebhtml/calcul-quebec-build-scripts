module-whatis "(Name___________) git"
module-whatis "(Version________) 1.8.1.1.348.g78eb407-NO_SETITIMER-patch"
module-whatis "(Description_Fr_) Git est un logiciel gratuit et libre de gestion de versions décentralié  (SCM)."
module-whatis "(Description_En_) Git is a free & open source, distributed version control system designed to handle everything from small to very large projects with speed and efficiency."
module-whatis "(Website________) http://git-scm.com/"
module-whatis "(Authorship_____) See: http://git-scm.com/about"
module-whatis "(Compiler_______) GCC 4.1.2 (system default)"
module-whatis "(Flags__________)"
module-whatis "(Dependencies___)"
module-whatis "(Notes_Fr_______)"
module-whatis "(Notes_En_______)"

set                synopsys          /software/apps/git/1.8.1.1.348.g78eb407-NO_SETITIMER-patch
prepend-path       PATH              $synopsys/bin
prepend-path       MANPATH           $synopsys/share/man
prepend-path       PYTHONPATH        $synopsys/lib/python2.4/site-packages
